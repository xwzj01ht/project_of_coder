F.addLog("mantpl:nav",{fanliLayerShow:"0100114000",faliLinkClick:"0100114001"});F.module("mantpl:nav/index",function(i,j,k){var l=$("#s_xmancard_nav");function m(){n();h()}function h(){if(window.s_session.userTips.supermanGuide==="off"){window.s_session.userTips.supermanGuide=false}if(window.s_session.userTips.supermanGuide||!window.s_session.userTips.supermanNavGuide||window.s_session.userTips.supermanNavGuide==="off"){return false}var a='<div class="nav-tips"><span>“我的导航”搬到<a>我的关注</a>里啦～</span><span class="nav-tips-closer"></span></div>';var b=$(a);b.appendTo(l.find(".s-nav-wrapper"));l.on("click",".nav-tips-closer, span a",function(c){if(/^[Aa]$/.test(c.target.tagName)){$("#s_menu_mine").trigger("click")}l.find(".s-nav-wrapper .nav-tips").remove();F.use("superman:common/user_attr",function(d){d.setAttr("supermanNavGuide","off")})});setTimeout(function(){l.find(".s-nav-wrapper .nav-tips").remove();F.use("superman:common/user_attr",function(c){c.setAttr("supermanNavGuide","off")})},4000);return true}function n(){$("#s_ctner_contents").delegate(".nav-block","click",function(d){if(s_session.curmod!="1"){return}var f=$(this).parents("[data-dir]").attr("data-dir");var c=$(this).attr("data-pos");var a=$(this).attr("data-log");var b=$(this).parent().attr("data-extra");if(a){a=/stype\:(\d)+;/g.exec(a);a=a&&a[1]}var e={title:$(this).attr("title")||"",url:$(this).attr("href")||"",dir:f||"热门导航",pos:c,stype:a,extra:b};if($(this).attr("data-fanliurl")&&s_session.userTips.navQuanyiTip){e.url=$(this).attr("data-fanliurl")}}).delegate(".nav-item","mouseenter mouseleave",function(a){if(a.type=="mouseenter"){if(s_session.userTips.navQuanyiTip){$(this).find(".rebate").show()}else{$(this).find(".rebate").hide()}}else{$(this).find(".rebate").hide()}}).delegate(".rebate","mouseenter mouseleave",function(a){if(a.type=="mouseenter"){$(this).addClass("rebate-hover")}else{$(this).removeClass("rebate-hover")}}).delegate(".rebate a","click",function(a){k.fire("faliLinkClick",{text:$(this).text()||"",url:$(this).attr("href")||""})}).delegate(".nav-block","click",function(a){if($(this).attr("data-fanliurl")&&s_session.userTips.navQuanyiTip){window.open($(this).attr("data-fanliurl"))}else{window.open($(this).attr("href"))}a.stopPropagation();a.preventDefault()}).delegate(".fanli-tag a","click",function(a){k.fire("faliLinkClick",{text:$(this).text()||"",url:$(this).attr("href")||""});a.stopPropagation()});k.listen("superman:common/user_attr",["setUserAttr"],function(a){setTimeout(function(){if(s_session.userTips.navQuanyiTip){$("#s_xmancard_nav .nav-block-wrapper").removeClass("normal-patern");$("#s_xmancard_nav .nav-block-wrapper").addClass("fanli-patern")}else{$("#s_xmancard_nav .nav-block-wrapper").removeClass("fanli-patern");$("#s_xmancard_nav .nav-block-wrapper").addClass("normal-patern")}},80)})}j.init=m});F.use("mantpl:nav/index",function(b){b.init()});